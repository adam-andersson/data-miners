import numpy as np


baskets = open("T10I4D100K.dat")
baskets = baskets.readlines()

store = [0 for i in range(200)]

for items in baskets:
    item_list = items.split(" ")

    for item in item_list:
        try:
            item = int(item)
            if item > len(store):
                diff = item - len(store)
                store += [0 for _ in range(diff)]
                store.append(1)
            else:
                store[item] += 1
        except: pass

